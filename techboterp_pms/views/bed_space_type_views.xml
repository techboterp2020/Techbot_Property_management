<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <!-- Bed space Birth Type Details-->
    <record id="bed_space_birth_type_tree_view" model="ir.ui.view">
        <field name="name">bed.space.birth.tree.view</field>
        <field name="model">birth.type</field>
        <field name="arch" type="xml">
            <tree string="Birth Type" sample="1" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_bed_birth_type" model="ir.actions.act_window">
        <field name="name">Bed Space Type </field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">birth.type</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a new document
            </p>
            <p>
                Also you will find here all the related documents and download it by clicking on any individual
                document.
            </p>
        </field>
    </record>
</odoo>
